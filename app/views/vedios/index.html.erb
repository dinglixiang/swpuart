<h1>所有的视频</h1>

<table>
  
<% @vedios.each do |vedio| %>
  <tr>
     <td id="myContent">
        
         Alternative content
     </td>
     <td>
       <%= vedio.name %>
     </td>   
  </tr>

  <tr>
     <td><%= link_to '播放', vedio %></td>
     <td><%= link_to 'Edit', edit_video_path(vedio) %></td>
     <td><%= link_to 'Destroy', vedio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>

  <script type="text/javascript" src="swfobject.js"></script>

<script type="text/javascript">  
  var so = new SWFObject('<%= vedio.avatar.url%>', 'mpl', '100', '100', '8');  
    
  so.addParam('allowfullscreen', 'true');
  so.addParam('play ', 'false');  
  so.addVariable('height', '360');  
  so.addVariable('width', '480');  
  
  so.write('myContent');  
</script>  
<% end %>
</table>


  
<br />

<%= link_to 'New Video', new_video_path %>
